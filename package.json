	"name": "bn-web",
	"version": "1.0.303",
	"private": true,
	"dependencies": {
		"@date-io/moment": "1.0.1",
		"@material-ui/core": "3.1.1",
		"@material-ui/icons": "^1.1.1",
		"@sentry/browser": "^4.6.4",
		"autoprefixer": "7.1.6",
		"axios": "^0.18.0",
		"babel-core": "6.26.0",
		"babel-jest": "20.0.3",
		"babel-loader": "7.1.2",
		"babel-preset-react-app": "^3.1.1",
		"babel-runtime": "6.26.0",
		"bn-api-node": "latest",
		"case-sensitive-paths-webpack-plugin": "2.1.1",
		"chalk": "1.1.3",
		"cloudinary-react": "^1.0.6",
		"css-loader": "0.28.7",
		"dotenv": "4.0.0",
		"dotenv-expand": "4.2.0",
		"eslint-loader": "1.9.0",
		"eslint-plugin-flowtype": "2.39.1",
		"extract-text-webpack-plugin": "3.0.2",
		"file-loader": "1.1.5",
		"fs-extra": "3.0.1",
		"html-webpack-plugin": "2.29.0",
		"jest": "20.0.4",
		"material-ui-pickers": "^2.2.1",
		"mobx": "^5.5.2",
		"mobx-react": "^5.3.5",
		"mobx-react-devtools": "^6.0.3",
		"moment": "^2.24.0",
		"moment-timezone": "^0.5.23",
		"object-assign": "4.1.1",
		"postcss-flexbugs-fixes": "3.2.0",
		"postcss-loader": "2.0.8",
		"promise": "8.0.1",
		"prop-types": "latest",
		"qrcode.react": "^0.8.0",
		"raf": "3.4.0",
		"react": "^16.6.0",
		"react-dev-utils": "^5.0.3",
		"react-dom": "^16.6.0",
		"react-ga": "^2.5.6",
		"react-grecaptcha": "^1.2.5",
		"react-helmet": "^5.2.0",
		"react-places-autocomplete": "^7.2.0",
		"react-router-dom": "^4.3.1",
		"react-select": "^2.1.1",
		"react-stripe-elements": "^2.0.3",
		"react-text-mask": "^5.4.3",
		"resolve": "1.6.0",
		"style-loader": "0.19.0",
		"sw-precache-webpack-plugin": "0.11.4",
		"url-loader": "0.6.2",
		"webpack": "3.8.1",
		"webpack-dev-server": "2.9.4",
		"webpack-manifest-plugin": "1.3.2",
		"whatwg-fetch": "2.0.3"
	},
	"scripts": {
		"start": "node scripts/start.js",
		"build": "node scripts/build.js",
		"test": "node scripts/test.js --env=jsdom",
		"prepare": "[ \"$NODE_ENV\" != \"production\" ] && git config core.hooksPath .githooks",
		"eslint-validate": "node_modules/eslint/bin/eslint.js -c .eslintrc --ignore-path=.eslintignore --ext .js ./ ",
		"eslint-fix": "node_modules/eslint/bin/eslint.js --fix -c .eslintrc --ignore-path=.eslintignore --ext .js ./ ",
		"release:patch": "npm version patch && git push origin && git push origin --tags",
		"lint": "npm run eslint-validate"
	},
	"devDependencies": {
		"babel-eslint": "^8.2.6",
		"babel-plugin-transform-decorators-legacy": "^1.3.5",
		"copy-webpack-plugin": "^4.6.0",
		"eslint": "^4.19.1",
		"eslint-config-react-app": "^2.1.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-jsx-a11y": "^6.1.2",
		"eslint-plugin-react": "^7.11.1",
		"react-scripts": "^2.1.3",
		"serve": "^10.0.2"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,mjs}"
		],
		"setupFiles": [
			"<rootDir>/config/polyfills.js"
		],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
			"<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
		],
		"testEnvironment": "node",
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
		],
		"moduleNameMapper": {
			"^react-native$": "react-native-web"
		},
		"moduleFileExtensions": [
			"web.js",
			"js",
			"json",
			"web.jsx",
			"jsx",
			"node",
			"mjs"
		]
	},
	"babel": {
		"plugins": [
			"transform-decorators-legacy"
		],
		"presets": [
			"react-app"
		]
	},
	"eslintConfig": {
		"extends": "react-app"
	}
}
